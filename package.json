{
    "description": "Discord.js Pagination",
    "author": "Ghex",
    "name": "@ghextercortes/djs-pagination",
    "main": "./dist/cjs/index.js",
    "types": "./dist/types/index.d.ts",
    "module": "./dist/mjs/index.js",
    "license": "MIT",
    "version": "3.1.0",
    "exports": {
        "require": "./dist/cjs/index.js",
        "import": "./dist/mjs/index.js"
    },
    "engines": {
        "node": ">=16.9.0"
    },
    "keywords": [
        "djs",
        "discord.js",
        "pagination",
        "reaction",
        "button"
    ],
    "homepage": "https://github.com/NotGhex/djs-pagination#readme",
    "scripts": {
        "clean": "rm -rf dist",
        "build": "yarn run clean && yarn run build:types && yarn run build:cjs && yarn run build:mjs",
        "build:types": "npx tsc -p ./tsconfig/tsconfig.types.json",
        "build:cjs": "npx tsc -p ./tsconfig/tsconfig.cjs.json",
        "build:mjs": "npx tsc -p ./tsconfig/tsconfig.mjs.json && echo '{ \"type\": \"module\" }' >> ./dist/mjs/package.json",
        "build:publish": "yarn run build && yarn publish --access public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NotGhex/djs-pagination.git"
    },
    "bugs": {
        "url": "https://github.com/NotGhex/djs-pagination/issues"
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "workspaces": [
        "./test"
    ],
    "devDependencies": {
        "@types/node": "^18.7.14",
        "discord.js": "^14.5.0",
        "typescript": "^4.8.2"
    },
    "peerDependencies": {
        "discord.js": "^14.5.0"
    },
    "packageManager": "yarn@3.2.3"
}
